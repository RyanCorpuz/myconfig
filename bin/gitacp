#!/usr/bin/sh

# ------------------------------------------------
# [Ryan Corpuz]	
# NAME
#		gitproj	-	Create github repo with current name
#
# SYNOPSIS
#		gitproj	[OPTION] [PROJECT NAME]
#
# DESCRIPTION
#		Creates github repo of specified name
#
#		-p
#			creates a private github repo
# ------------------------------------------------

package=gitacp

if [ -z $1 ]; then
	echo "$package: missing operand"
	exit 1
fi

git add .
git commit -m "$1"
git push -u origin master

# what up
